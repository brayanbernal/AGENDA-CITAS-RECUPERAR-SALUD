						<li>
							<a href="~/ACCESO/Login" style="color:white">
								SALIR
							</a>
						</li>
					</ul>
			</div>
		</div>
	</div>
	<div class="container body-content">
		@RenderBody()
		<hr />
		<footer>
			<p style="text-align:center">&copy; @DateTime.Now.Year - RECUPERAR SALUD</p>
		</footer>
